             HIY "ÎûÎûĞ¦ÁË¼¸Éù¡£\n" NOR,
             me, this_object());
              
     me->delete("quest");
     me->delete("xquest");
     me->delete("changanjob");

     me->add("balance",-2000000);
     return 1;
}


