int ask_zhen()
{
    command("say ËÍÎÒÒ»°ÑÄã×Ô¼ºÁ·³öµÄ¾Å¼¶±øÆ÷ÎÒ¾Í¸øÄã¸ö»ÆÁºÕí¡£");
    this_player()->delete_temp("want_dan");
    this_player()->set_temp("want_zhen",1);
    return 1;
}

