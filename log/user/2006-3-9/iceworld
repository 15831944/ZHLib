int accept_object(object me, object ob)
{
    object itm;
    if (!ob->is_item_make()) return 0;  
    if (ob->item_owner() != me->query("id"))
    {
       command("kick " + me->query("id"));
       command("say ÕâÓÖ²»ÊÇÄãµÄ¶«Î÷£¬µ½Ê±ºòËüµÄÖ÷ÈËÒªÀ´ÌÖ»ØÔõÃ´°ì£¿");
       return 0;
