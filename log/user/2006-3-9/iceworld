        if ( me->query_skill("unarmed", 1) < 40 ) me->set_skill("unarmed",40);
        if ( me->query_skill("strike", 1) < 40 ) me->set_skill("strike",40);
        if ( me->query_skill("sword", 1) < 40 ) me->set_skill("sword",40);
